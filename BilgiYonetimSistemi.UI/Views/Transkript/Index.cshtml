                                            <td>
                                                @bolumders.BolumunDersi.DersAdi
                                            </td>
                                            <td class="text-center">
                                                <span></span>
                                            </td>
                                            <td class="text-center">
                                                @{
                                                    int? birinciVizePuanı = Model.FirstOrDefault(x => x.DersID == bolumders.BolumunDersi.DersID).OgrenciDerslerDonemlerinNotlari.FirstOrDefault(x => x.SinavID == 1).Puan;

                                                    int? ikinciVizePuanı = Model.FirstOrDefault(x => x.DersID == bolumders.BolumunDersi.DersID).OgrenciDerslerDonemlerinNotlari.FirstOrDefault(x => x.SinavID == 2).Puan;

                                                    int? finalPuanı = Model.FirstOrDefault(x => x.DersID == bolumders.BolumunDersi.DersID).OgrenciDerslerDonemlerinNotlari.FirstOrDefault(x => x.SinavID == 3).Puan;

                                                    double? finalliSonuc = birinciVizePuanı * 0.2 + ikinciVizePuanı * 0.2 + finalPuanı * 0.6;
                                                    double? butunlemePuanı = Model.FirstOrDefault(x => x.DersID == bolumders.BolumunDersi.DersID).OgrenciDerslerDonemlerinNotlari.FirstOrDefault(x => x.SinavID == 4).Puan;

                                                    double? butluSonuc = birinciVizePuanı * 0.2 + ikinciVizePuanı * 0.2 + butunlemePuanı * 0.6;

                                                    if (butunlemePuanı == 0)
                                                    {

                                                        if (finalliSonuc < 45)
                                                        {
                                                            <span>FF</span>
                                                            toplamAgirlikli += +0 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 45 && finalliSonuc < 50)
                                                        {
                                                            <span>DD</span>
                                                            toplamAgirlikli += +1 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 50 && finalliSonuc < 60)
                                                        {
                                                            <span>DC</span>
                                                            toplamAgirlikli += 1.5 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 60 && finalliSonuc < 70)
                                                        {
                                                            <span>CC</span>
                                                            toplamAgirlikli += 2 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 70 && finalliSonuc < 80)
                                                        {
                                                            <span>CB</span>
                                                            toplamAgirlikli += 2.5 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 80 && finalliSonuc < 85)
                                                        {
                                                            <span>BB</span>
                                                            toplamAgirlikli += 3 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (finalliSonuc >= 85 && finalliSonuc < 90)
                                                        {
                                                            toplamAgirlikli += 3.5 * bolumders.BolumunDersi.DersKredisi;
                                                            <span>BA</span>
                                                        }
                                                        else if (finalliSonuc >= 90 || butluSonuc >= 90)
                                                        {
                                                            <span>AA</span>
                                                            toplamAgirlikli += 4 * bolumders.BolumunDersi.DersKredisi;
