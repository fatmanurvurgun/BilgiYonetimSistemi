    @{
        int toplamKrediSayisi = 0;
        double genelToplamAgirlikli = 0;
        int donemSonuToplamKredi = 0;
        double toplamAgirlikli = 0;
    }

    @for (int i = 1; i < (int)ViewBag.OgrencininDonemSayisi; i++)
    {
        donemSonuToplamKredi = 0;
        toplamAgirlikli = 0;
