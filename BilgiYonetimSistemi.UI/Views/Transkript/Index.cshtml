                                                        {
                                                            <span>FF</span>
                                                            toplamAgirlikli += 0 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 45 && butluSonuc < 50)
                                                        {
                                                            <span>DD</span>
                                                            toplamAgirlikli += 1 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 50 && butluSonuc < 60)
                                                        {
                                                            <span>DC</span>
                                                            toplamAgirlikli += 1.5 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 60 && butluSonuc < 70)
                                                        {
                                                            <span>CC</span>
                                                            toplamAgirlikli += 2 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 70 && butluSonuc < 80)
                                                        {
                                                            <span>CB</span>
                                                            toplamAgirlikli += 2.5 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 80 && butluSonuc < 85)
                                                        {
                                                            <span>BB</span>
                                                            toplamAgirlikli += 3 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 85 && butluSonuc < 90)
                                                        {
                                                            <span>BA</span>
                                                            toplamAgirlikli += 3.5 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                        else if (butluSonuc >= 90)
                                                        {
                                                            <span>AA</span>
                                                            toplamAgirlikli += 4 * bolumders.BolumunDersi.DersKredisi;
                                                        }
                                                    }
                                                }
                                            </td>
                                            <td class="text-center">
                                                @bolumders.BolumunDersi.DersKredisi
                                            </td>
                                        </tr>
                                    }
                                }
                            }
                        </tbody>

                        <tfoot>
                            @{genelToplamAgirlikli += toplamAgirlikli;
                                toplamKrediSayisi += donemSonuToplamKredi;
                                <tr style="background-color:#dddddd !important;">
                                    <td colspan="2" class="text-right">
                                        <strong>Toplam AKTS Kredi</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>Toplam Ağırlıklı</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>Ortalama</strong>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="background-color:#f9f9f9;">
                                        Dönem Sonu:
                                    </td>
                                    <td class="text-center">
                                        <strong>@donemSonuToplamKredi</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>@toplamAgirlikli</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>@((toplamAgirlikli / donemSonuToplamKredi).ToString("G3"))</strong> 
                                    </td>
                                </tr>
                                <tr>
                                    <td style="background-color:#f9f9f9;">
                                        Genel:
                                    </td>
                                    <td class="text-center">
                                        <strong>@toplamKrediSayisi</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>@genelToplamAgirlikli</strong>
                                    </td>
                                    <td class="text-center">
                                        <strong>@((genelToplamAgirlikli / toplamKrediSayisi).ToString("G3"))</strong>
                                    </td>
                                </tr>
                            }
                        </tfoot>
                    </table>
                </div>
            </div>

        </div>
    }
</div>
